{
  "homepage": "https://galacticcouncil.github.io/basilisk-ui/",
  "name": "basilisk-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@polkadot/api": "^6.4.2",
    "@polkadot/extension-dapp": "^0.40.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.176",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/use-persisted-state": "^0.3.0",
    "add-days": "^2.0.1",
    "babel-loader": "8.1.0",
    "bootstrap": "^5.1.1",
    "bootstrap-icons": "^1.6.0",
    "chart.js": "^3.5.1",
    "chartjs-adapter-moment": "^1.0.0",
    "chartjs-plugin-annotation": "^1.0.2",
    "classnames": "^2.3.1",
    "constate": "^3.3.0",
    "date-fns": "^2.25.0",
    "gql": "^1.1.2",
    "graphql.macro": "^1.4.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "percent-change": "^1.0.1",
    "react": "^17.0.2",
    "react-apollo-network-status": "^5.1.0",
    "react-chartjs-2": "^3.0.5",
    "react-countup": "^6.0.0",
    "react-currency-input-field": "^3.5.1",
    "react-dom": "^17.0.2",
    "react-intl": "^5.20.12",
    "react-multi-provider": "^0.1.5",
    "react-scripts": "4.0.3",
    "react-use": "^17.3.1",
    "save": "^2.4.0",
    "typeface-roboto": "^1.1.13",
    "typescript": "^4.1.2",
    "use-count-up": "^3.0.1",
    "use-persisted-state": "^0.3.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "FAST_REFRESH=false react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns 'node_modules/(?!@polkadot)/'",
    "test:ci": "CI=true react-scripts test --transformIgnorePatterns 'node_modules/(?!@polkadot)/'",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache --transformIgnorePatterns 'node_modules/(?!@polkadot)/'",
    "eject": "react-scripts eject",
    "lint": "eslint",
    "commit": "git-cz",
    "preci": "rm -fr node_modules",
    "ci": "yarn install --frozen-lockfile",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "translations:extract": "formatjs extract 'src/**/*.ts*' --out-file src/lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --ignore **/*.d.ts",
    "translations:compile": "formatjs compile src/lang/en.json --out-file src/compiled-lang/en.json",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "*"
        ],
        "rules": {
          "react/style-prop-object": [
            1,
            {
              "allow": [
                "FormattedNumber"
              ]
            }
          ]
        }
      }
    ]
  },
  "browserslist": [],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "babel": {
    "plugins": [
      [
        "formatjs",
        {
          "idInterpolationPattern": "[sha512:contenthash:base64:6]",
          "ast": true
        }
      ]
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@formatjs/cli": "^4.3.1",
    "@graphql-codegen/cli": "2.2.1",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.2.4",
    "@graphql-codegen/typescript-operations": "2.1.8",
    "@graphql-codegen/typescript-react-apollo": "3.1.6",
    "@storybook/addon-actions": "^6.3.11",
    "@storybook/addon-essentials": "^6.3.11",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-links": "^6.3.11",
    "@storybook/node-logger": "^6.3.11",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.11",
    "@testing-library/react-hooks": "^7.0.2",
    "babel-plugin-formatjs": "^10.3.9",
    "bignumber.js": "^9.0.1",
    "chai": "^4.3.4",
    "commitizen": "^4.2.4",
    "css-loader": "5.2.6",
    "cz-conventional-changelog": "^3.2.0",
    "node-sass": "^6.0.1",
    "sass": "^1.42.1",
    "sass-loader": "10.1.1",
    "style-loader": "2.0.0",
    "wait-for-expect": "^3.0.2"
  }
}
